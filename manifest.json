{
  "manifest_version": 3,
  "name": "Keyword Density Checker",
  "version": "1.0",
  "description": "Supercharge your SEO endeavors with our cutting-edge Keyword Density Checker Chrome extension!",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
    
